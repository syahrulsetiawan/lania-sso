{
  "version": 2,
  "installCommand": "npm ci",
  "buildCommand": "npm run build",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "memory": 1024
    }
  },
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS", "HEAD"]
    }
  ]
}
